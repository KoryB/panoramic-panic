            <kind>1</kind>
            <string>/// Ball Update

//show_debug_message(state)

if (state == "death")
{
    if (image_index &gt;= image_number - 3)
    {
        image_speed = 0;
        image_index = image_number - 1;
    }
    
    exit
}

if (state == "idle")
{    
    scr_ball_snap_window();
    
    oldx = x;
    oldy = y;
    
    scr_ball_do_collision();
    
    if (abs(oldx - x) &gt;= 3 || abs(oldy - y) &gt;= 3)
    {
        audio_play_sound(s_bounce, 0, false);
        state = "moving"
        dx = x - oldx;
        dy = y - oldy;
    }
}
else if (state == "moving" || state == "zooming")
{
    newx = x + dx;
    newy = y + dy;
    
    oldx = x;
    oldy = y;
    
    scr_ball_do_collision()
    
    if (scr_window_rectangle_outside(bbox_left, bbox_top, bbox_right, bbox_bottom))
    {
        scr_ball_snap_window();
        
        state = "idle";
        lastGoer = noone;
        sprite_index = spr_Ball1;
    }
    
    if ((oldx == x &amp;&amp; dx != 0) || (oldy == y &amp;&amp; dy != 0))
    {
        state = "idle";
        lastGoer = noone;
        sprite_index = spr_Ball1;
    }
}

goer = instance_place(x, y, obj_Goer)

if (goer != noone and goer != lastGoer)
{
    scr_ball_go(goer);
    
    if (state != "zooming")
    {
        state = "moving"
    }
       
    if (goer.object_index == obj_Zoomer)
    {
        state = "zooming"
        
        audio_play_sound(s_electric, 5, false);
        
        sprite_index = spr_BallZoom
        image_index = 0
        image_speed = 1
    }
}

if (collision_point(mouse_x, mouse_y, id, true, false))
{
    show_debug_message("inside &lt;" + string(mouse_x) + ", " + string(mouse_y) + "&gt;");
}

</string>
